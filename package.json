{
  "name": "vrf-vuetify",
  "version": "0.41.1",
  "author": "Dmitry Sadovsky <dimailn9@gmail.com>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/lib.vue.config.js vue-cli-service build --target lib ./src/index.js",
    "lint": "vue-cli-service lint"
  },
  "main": "dist/vrf-vuetify.common.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/insales/vrf-vuetify"
  },
  "dependencies": {
    "clipboard": "^2.0.11",
    "core-js": "^3.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "vue-cli-plugin-vuetify": "~2.5.8",
    "vue-template-compiler": "^2.6.14",
    "vuetify": "^2.6.0",
    "vue": "^2.6.14",
    "vrf": "^0.57.0"
  },
  "peerDependencies": {
    "vuetify": "^2.6.0",
    "vue": "^2.6.14",
    "vrf": "^0.57.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/multi-word-component-names": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
